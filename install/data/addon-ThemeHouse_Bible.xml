    <template title="th_bible.css" version_id="1433786497" version_string="1.1.1"><![CDATA[.bible sup.versenum, .bbCodeBibleVerse sup.versenum
{
	vertical-align: super;
	font-size: inherit;
	font-size: 70%;
}

.bible sup.chapternum
{
	font-size: 24px;
	font-weight: bold;
	position: relative;
	line-height: .8em;
	bottom: -0.1em;
	left: 0;
	text-indent: 0;
}

.bible p
{
	padding-bottom: 10px;
}

.bible span a
{
	text-decoration: none;
	color: inherit;
}

.bible span a:hover
{
	text-decoration: none;
	color: inherit;
	background-color: {xen:property inlineMod};
}

.verseTooltip.bible .messageText
{
	font-size: 10pt;
}

.verseTooltip.bible > span.muted
{
	font-size: 8pt;
	display: block;
	margin-top: 10px;
}

/* ***************************** */

#VerseTooltip
{
	display: none;
}

.xenVerseTooltip
{
	{xen:property previewTooltip}
	
	display: none;	
	z-index: 15000;
	cursor: default;
	
	border-color: {xen:helper rgba, {xen:property previewTooltip.border-color}, 0.5};
}

	.xenVerseTooltip .arrow
	{
		{xen:property previewTooltipArrowOuter}
		
		_display: none;
	}
	
		.xenVerseTooltip .arrow span
		{
			{xen:property previewTooltipArrowInner}
		}

	.xenVerseTooltip .section,
	.xenVerseTooltip .sectionMain,
	.xenVerseTooltip .primaryContent,
	.xenVerseTooltip .secondaryContent
	{
		margin: 0;
	}
	
		.xenVerseTooltip .verseContent
		{
			overflow: hidden; zoom: 1;
			min-height: 1em;
		}]]></template>
