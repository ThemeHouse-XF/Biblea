	data-end="{$endPage}"
	data-last="{$pageTotal}"
	data-sentinel="{$pageNumberSentinel}"
	data-baseurl="{xen:link bible, {xen:array 'url_portion={$linkType}', 'chapter={$linkData}', 'verse={$pageNumberSentinel}'}, '_params={$linkParams}'}">
	
	<span class="pageNavHeader">{xen:phrase th_verse_x_of_y_bible, 'current={$currentPage}', 'total={$pageTotal}'}</span>
	
	<nav>
		<xen:if is="{$prevPage}">
			<a href="{xen:link bible, {xen:array 'url_portion={$linkType}', 'chapter={$linkData}', 'verse={xen:if "{$prevPage} > 1", $prevPage, 1}'}, '_params={$linkParams}'}" class="text">&lt; {xen:phrase prev}</a>
			<xen:container var="$head.prev"><link rel="prev" href="{xen:link bible, {xen:array 'url_portion={$linkType}', 'chapter={$linkData}', 'verse={xen:if "{$prevPage} > 1", $prevPage, 1}'}, '_params={$linkParams}'}" /></xen:container>
		</xen:if>
		
		<a href="{xen:link bible, {xen:array 'url_portion={$linkType}', 'chapter={$linkData}', 'verse=1'}, '_params={$linkParams}'}" class="{xen:if '1 == {$currentPage}', 'currentPage '}" rel="start">1</a>
		
		<xen:if is="{$pageTotal} > {$scrollThreshold}">
			<a class="PageNavPrev {xen:if '{$startPage} <= 2', hidden}">&larr;</a> <span class="scrollable"><span class="items">
		</xen:if>
		
		<xen:foreach loop="{$pages}" value="{$pageNumber}">
			<a href="{xen:link bible, {xen:array 'url_portion={$linkType}', 'chapter={$linkData}', 'verse={$pageNumber}'}, '_params={$linkParams}'}" class="{xen:if '{$pageNumber} == {$currentPage}', 'currentPage'}">{$pageNumber}</a>
		</xen:foreach>
		
		<xen:if is="{$pageTotal} > {$scrollThreshold}">
			</span></span> <a class="PageNavNext {xen:if '{$endPage} + 1 >= {$pageTotal}', hidden}">&rarr;</a>
		</xen:if>
		
		<a href="{xen:link bible, {xen:array 'url_portion={$linkType}', 'chapter={$linkData}', 'verse={$pageTotal}'}, '_params={$linkParams}'}" class="{xen:if '{$pageTotal} == {$currentPage}', 'currentPage'}">{$pageTotal}</a>
		
		<xen:if is="{$nextPage}">
			<a href="{xen:link bible, {xen:array 'url_portion={$linkType}', 'chapter={$linkData}', 'verse={$nextPage}'}, '_params={$linkParams}'}" class="text">{xen:phrase next} &gt;</a>
			<xen:container var="$head.next"><link rel="next" href="{xen:link bible, {xen:array 'url_portion={$linkType}', 'chapter={$linkData}', 'verse={$nextPage}'}, '_params={$linkParams}'}" /></xen:container>
		</xen:if>
		
		<a href="{xen:link bible, {xen:array 'url_portion={$linkType}', 'chapter={$linkData}'}, '_params={$linkParams}'}" class="text">{xen:phrase th_view_in_context_bible}</a>
	</nav>
</div>
</xen:if>]]></template>
    <template title="th_verse_tooltip_bible" version_id="1433786497" version_string="1.1.1"><![CDATA[<div id="VerseTooltip">
