    <template title="th_book_edit_bible"><![CDATA[<xen:title>{xen:if '{$book.book_id}', '{xen:phrase th_edit_book_bible}: {$book.title}', '{xen:phrase th_create_new_book_bible}'}</xen:title>

<xen:h1>{xen:if '{$book.book_id}', '{xen:phrase th_edit_book_bible}: <em>{$book.title}</em>', '{xen:phrase th_create_new_book_bible}'}</xen:h1>

<xen:if is="{$book.book_id}">
	<xen:navigation>
		<xen:breadcrumb href="{xen:adminlink 'bible-books'}#{xen:helper listitemid, $book.book_id}">{$book.title}</xen:breadcrumb>
	</xen:navigation>
</xen:if>

<xen:form action="{xen:adminlink 'bible-books/save', $book}" class="AutoValidator" data-redirect="on">
	<xen:textboxunit label="{xen:phrase title}:" name="title" value="{$book.title}" data-liveTitleTemplate="{xen:if {$book.book_id},
		'{xen:phrase th_edit_book_bible}: <em>%s</em>',
		'{xen:phrase th_create_new_book_bible}: <em>%s</em>'}" />

	<xen:textboxunit label="{xen:phrase url_portion}:" explain="{xen:phrase th_url_portion_explain_bible}" name="url_portion" value="{$book.url_portion}" />

	<xen:selectunit label="{xen:phrase th_section_bible}:" explain="{xen:phrase th_section_explain_bible}" name="section">
		<xen:foreach loop="$sectionTitles" key="$section" value="$sectionTitle">
			<xen:option value="{$section}" label="{$sectionTitle}" selected="{$book.section} == {$section}" />
		</xen:foreach>
	</xen:selectunit>

	<xen:spinboxunit label="{xen:phrase display_order}:" name="priority" value="{$book.priority}" />

	<xen:textboxunit label="{xen:phrase th_allowed_names_bible}:" explain="{xen:phrase th_book_names_explain_bible}" name="book_names" value="{$bookNames}" />

	<xen:submitunit>
