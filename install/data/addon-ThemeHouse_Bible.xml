<xen:if is="{$xenOptions.boardDescription}"><xen:container var="$head.description">
	<meta name="description" content="{$title} - {$bible.title}" /></xen:container></xen:if>
<xen:container var="$head.openGraph">
	<xen:include template="open_graph_meta">
		<xen:set var="$url">{xen:link 'canonical:bible', {$navParams}, 'bible_id={$bible.bible_id}'}</xen:set>
		<xen:set var="$title">{$xenOptions.boardTitle}</xen:set>
		<xen:set var="$description">{$title} - {$bible.title}</xen:set>
		<xen:set var="$ogType">website</xen:set>
	</xen:include></xen:container>
	
<xen:navigation>
	<xen:breadcrumb href="{xen:link 'full:bible', '', 'bible_id={$bible.bible_id}'}">{$bible.title}</xen:breadcrumb>
	<xen:breadcrumb href="{xen:link 'full:bible', {xen:array 'url_portion={$firstVerse.section_url_portion}'}, 'bible_id={$bible.bible_id}'}">{$firstVerse.section_title}</xen:breadcrumb>
	<xen:breadcrumb href="{xen:link 'full:bible', {xen:array 'url_portion={$firstVerse.url_portion}'}, 'bible_id={$bible.bible_id}'}">{$firstVerse.book_title}</xen:breadcrumb>
	<xen:breadcrumb href="{xen:link 'full:bible', {xen:array 'url_portion={$firstVerse.url_portion}', 'chapter={$firstVerse.chapter}'}, 'bible_id={$bible.bible_id}'}">{xen:phrase th_chapter_x_bible, 'chapter={$firstVerse.chapter}'}</xen:breadcrumb>
</xen:navigation>

<xen:require css="th_bible.css" />

<div class="pageNavLinkGroup">
	<div class="linkGroup">
		<div class="Popup filterLink JsOnly">
			<a rel="Menu">{xen:phrase th_books_bible}</a>
			<div class="Menu bibleBookMenu" id="BibleBookMenu"
				data-contentsrc="{xen:link bible/books, '', 'book_id={$firstVerse.book_id}', 'bible_id={$bible.bible_id}'}"
				data-contentdest="#BibleBookMenu > .primaryContent"
				data-insertfn="replaceAll"
			>
				<div class="primaryContent">{xen:phrase loading}...</div>
			</div>
		</div>
		<div class="Popup filterLink JsOnly">
			<a rel="Menu">{xen:phrase th_translations_bible}</a>
			<div class="Menu bibleBookMenu" id="BibleMenu"
				data-contentsrc="{xen:link bible/translations, '', 'book_id={$firstVerse.book_id}', 'chapter={$firstVerse.chapter}', 'bible_id={$bible.bible_id}'}"
				data-contentdest="#BibleMenu > .primaryContent"
				data-insertfn="replaceAll"
			>
				<div class="primaryContent">{xen:phrase loading}...</div>
			</div>
		</div>
	</div>

	<xen:if is="{$verseCount}">
		<xen:pagenav link="{$firstVerse.url_portion}" linkparams="{xen:array 'bible_id={$bible.bible_id}'}" linkdata="{$firstVerse.chapter}" page="{$firstVerse.verse}" perpage="1" total="{$verseCount}" options="{xen:array 'template=th_verse_nav_bible'}" />
	<xen:else />
		<xen:pagenav link="{$firstVerse.url_portion}" linkparams="{xen:array 'bible_id={$bible.bible_id}'}" page="{$firstVerse.chapter}" perpage="1" total="{$chapterCount}" options="{xen:array 'template=th_chapter_nav_bible'}" />
	</xen:if>
</div>

<div class="primaryContent messageText bible">
	<p>
	<xen:foreach loop="$verses" key="$verseId" value="$verse">
		<xen:if is="{$verse.pe} || {$verse.samekh}">
			</p>
			<p>
		</xen:if>
		<span id="verse-{$verseId}" class="{$verse.verse}">
			<xen:if is="{xen:count $verses} > 1"><a href="{xen:link 'full:bible', {xen:array 'url_portion={$verse.url_portion}', 'chapter={$verse.chapter}', 'verse={$verse.verse}'}, 'bible_id={$bible.bible_id}'}"></xen:if>
			<xen:if is="{$verse.verse} > 1"><sup class="versenum">{$verse.verse}</sup><xen:else /><sup class="chapternum">{$verse.chapter}</sup></xen:if>
			{$verse.text}
			<xen:if is="{xen:count $verses} > 1"></a></xen:if>
		</span>
		<xen:if is="{$verse.paragraph_break}">
			</p>
			<p>
		</xen:if>
	</xen:foreach>
	</p>
</div>

<div class="pageNavLinkGroup">
	<xen:pagenav link="{$firstVerse.url_portion}" linkparams="{xen:array 'bible_id={$bible.bible_id}'}" page="{$firstVerse.chapter}" perpage="1" total="{$chapterCount}" options="{xen:array 'template=th_chapter_nav_bible'}" />
	<xen:if is="{$verseCount}">
		<xen:pagenav link="{$firstVerse.url_portion}" linkparams="{xen:array 'bible_id={$bible.bible_id}'}" linkdata="{$firstVerse.chapter}" page="{$firstVerse.verse}" perpage="1" total="{$verseCount}" options="{xen:array 'template=th_verse_nav_bible'}" />
	</xen:if>
</div>

<xen:sidebar>
	<xen:if is="{$posts}">
		<div class="section profilePostList">
			<div class="secondaryContent">
				<h3><a href="{xen:link search/results, '', '_params={xen:array 'q="{$title}"', 't=post'}'}">{xen:phrase th_related_posts_bible}</a></h3>
				<xen:include template="th_sidebar_post_list_bible" />
			</div>
		</div>
	</xen:if>
	
	<xen:include template="sidebar_share_page">
		<xen:set var="$url">{xen:link 'canonical:bible', {$navParams}, 'bible_id={$bible.bible_id}'}</xen:set>
	</xen:include>
</xen:sidebar>]]></template>
    <template title="th_chapter_nav_bible" version_id="1433500490" version_string="1.0.0"><![CDATA[<xen:edithint template="page_nav.css" />

<xen:if is="{$pageTotal} > 1">
<div class="PageNav"
	data-page="{$currentPage}"
	data-range="{$range}"
	data-start="{$startPage}"
	data-end="{$endPage}"
	data-last="{$pageTotal}"
	data-sentinel="{$pageNumberSentinel}"
	data-baseurl="{xen:link bible, {xen:array 'url_portion={$linkType}', 'chapter={$pageNumberSentinel}'}, '_params={$linkParams}'}">
	
	<span class="pageNavHeader">{xen:phrase th_chapter_x_of_y_bible, 'current={$currentPage}', 'total={$pageTotal}'}</span>
	
	<nav>
		<xen:if is="{$prevPage}">
			<a href="{xen:link bible, {xen:array 'url_portion={$linkType}', 'chapter={xen:if "{$prevPage} > 1", $prevPage}'}, '_params={$linkParams}'}" class="text">&lt; {xen:phrase prev}</a>
			<xen:container var="$head.prev"><link rel="prev" href="{xen:link bible, {xen:array 'url_portion={$linkType}', 'chapter={xen:if "{$prevPage} > 1", $prevPage}'}, '_params={$linkParams}'}" /></xen:container>
		</xen:if>
		
		<a href="{xen:link bible, {xen:array 'url_portion={$linkType}'}, '_params={$linkParams}'}" class="{xen:if '1 == {$currentPage}', 'currentPage '}" rel="start">1</a>
		
		<xen:if is="{$pageTotal} > {$scrollThreshold}">
			<a class="PageNavPrev {xen:if '{$startPage} <= 2', hidden}">&larr;</a> <span class="scrollable"><span class="items">
		</xen:if>
		
		<xen:foreach loop="{$pages}" value="{$pageNumber}">
			<a href="{xen:link bible, {xen:array 'url_portion={$linkType}', 'chapter={$pageNumber}'}, '_params={$linkParams}'}" class="{xen:if '{$pageNumber} == {$currentPage}', 'currentPage'}">{$pageNumber}</a>
		</xen:foreach>
		
		<xen:if is="{$pageTotal} > {$scrollThreshold}">
			</span></span> <a class="PageNavNext {xen:if '{$endPage} + 1 >= {$pageTotal}', hidden}">&rarr;</a>
		</xen:if>
		
		<a href="{xen:link bible, {xen:array 'url_portion={$linkType}', 'chapter={$pageTotal}'}, '_params={$linkParams}'}" class="{xen:if '{$pageTotal} == {$currentPage}', 'currentPage'}">{$pageTotal}</a>
		
		<xen:if is="{$nextPage}">
			<a href="{xen:link bible, {xen:array 'url_portion={$linkType}', 'chapter={$nextPage}'}, '_params={$linkParams}'}" class="text">{xen:phrase next} &gt;</a>
			<xen:container var="$head.next"><link rel="next" href="{xen:link bible, {xen:array 'url_portion={$linkType}', 'chapter={$nextPage}'}, '_params={$linkParams}'}" /></xen:container>
		</xen:if>
	</nav>
</div>
</xen:if>]]></template>
