<xen:navigation>
	<xen:breadcrumb href="{xen:adminlink 'bibles/edit', $bible}">{$bible.name}</xen:breadcrumb>
</xen:navigation>

<xen:require css="delete_confirmation.css" />

<xen:form action="{xen:adminlink 'bibles/delete', $bible}" class="deleteConfirmForm formOverlay">
	<p>{xen:phrase th_please_confirm_want_to_delete_bible_bible}:</p>
	<strong><a href="{xen:adminlink 'bibles/edit', $bible}">{$bible.name}</a></strong>

	<xen:submitunit save="{xen:phrase th_delete_bible_bible}" />

	<input type="hidden" name="_xfConfirm" value="1" />
</xen:form>]]></template>
    <template title="th_bible_edit_bible"><![CDATA[<xen:title>{xen:if '{$bible.bible_id}', '{xen:phrase th_edit_bible_bible}: {$bible.title}', '{xen:phrase th_create_new_bible_bible}'}</xen:title>

<xen:h1>{xen:if '{$bible.bible_id}', '{xen:phrase th_edit_bible_bible}: <em>{$bible.title}</em>', '{xen:phrase th_create_new_bible_bible}'}</xen:h1>

<xen:if is="{$bible.bible_id}">
	<xen:navigation>
		<xen:breadcrumb href="{xen:adminlink 'bibles'}#{xen:helper listitemid, $bible.bible_id}">{$bible.name}</xen:breadcrumb>
	</xen:navigation>
</xen:if>

<xen:form action="{xen:adminlink 'bibles/save', $bible}" class="AutoValidator" data-redirect="on">
	<xen:textboxunit label="{xen:phrase th_name_bible}:" explain="{xen:phrase th_name_explain_bible}" name="name" value="{$bible.name}" data-liveTitleTemplate="{xen:if {$bible.bible_id},
		'{xen:phrase th_edit_bible_bible}: <em>%s</em>',
		'{xen:phrase th_create_new_bible_bible}: <em>%s</em>'}" />
		
	<xen:if is="!{$bible.bible_id}">
		<xen:textboxunit label="{xen:phrase th_bible_id_bible}:" name="bible_id" />
	</xen:if>

	<xen:textboxunit label="{xen:phrase title}:" name="title" value="{$bible.title}" />

	<xen:textboxunit name="description" value="{$template.template}" label="{xen:phrase description}:" hint="{xen:phrase you_may_use_html}" explain="{xen:phrase th_description_explain_bible}" rows="2" inputclass="Elastic" />
			
	<xen:textboxunit label="{xen:phrase th_copyright_bible}:" explain="{xen:phrase th_copyright_explain_bible}" name="copyright" value="{$bible.copyright}" />

	<xen:textboxunit label="{xen:phrase th_abbreviation_bible}:" explain="{xen:phrase th_abbreviation_explain_bible}" name="abbreviation" value="{$bible.abbreviation}" />

	<xen:textboxunit label="{xen:phrase th_language_bible}:" explain="{xen:phrase th_language_explain_bible}" name="language" value="{$bible.language}" />

	<xen:textboxunit label="{xen:phrase th_note_bible}:" explain="{xen:phrase th_note_explain_bible}" name="note" value="{$bible.note}" />

	<xen:submitunit>
		<input type="submit" name="saveexit" value="{xen:phrase save_and_exit}" accesskey="e" class="button primary" id="saveExitButton" />
		<input type="submit" name="reload" value="{xen:phrase save_changes}" accesskey="s" class="button" id="saveReloadButton" data-ajaxvalue="{xen:phrase save_all_changes}" />
		<xen:if is="{$bible.bible_id}">
			<input type="button" value="{xen:phrase th_delete_bible_bible}..." accesskey="d" class="button OverlayTrigger"
				data-href="{xen:adminlink bibles/delete, $bible}" />
		</xen:if>
	</xen:submitunit>
</xen:form>]]></template>
    <template title="th_bible_import_bible"><![CDATA[<xen:title>{xen:phrase th_import_bible_bible}</xen:title>

<xen:form action="{xen:adminlink bibles/import}" upload="true">

	<xen:uploadunit label="{xen:phrase th_import_from_uploaded_zip_file_bible}:" name="upload" explain="{xen:phrase th_bibles_can_be_imported_from_tub_bible}" />

	<xen:submitunit save="{xen:phrase th_import_bible_bible}" />

	<input type="hidden" name="_xfConfirm" value="1" />
	
</xen:form>]]></template>
    <template title="th_bible_list_bible"><![CDATA[<xen:title>{xen:phrase th_bibles_bible}</xen:title>

<xen:h1>{xen:phrase th_bibles_bible}</xen:h1>

<xen:topctrl>
    <a href="{xen:adminlink 'bibles/add'}" class="button">+ {xen:phrase th_create_new_bible_bible}</a>
    <a href="{xen:adminlink 'bibles/import'}" class="button">{xen:phrase th_import_bible_bible}</a>
</xen:topctrl>

<xen:require css="filter_list.css" />
<xen:require js="js/xenforo/filter_list.js" />

<xen:form action="{xen:adminlink 'bibles/toggle'}" class="section AutoValidator">

    <xen:if is="{$bibles}">
        <h2 class="subHeading">
            <xen:include template="filter_list_controls" />
            {xen:phrase th_bibles_bible}
        </h2>

        <ol class="FilterList">
            <xen:foreach loop="$bibles" key="$bibleId" value="$bible">
                <xen:listitem id="{$bibleId}"
                    label="{$bible.name}"
                    snippet="{$bible.copyright}"
                    href="{xen:adminlink 'bibles/edit', $bible}"
                    delete="{xen:adminlink 'bibles/delete', $bible}"
                    deletehint="{xen:phrase delete}">
                </xen:listitem>
            </xen:foreach>
        </ol>

        <p class="sectionFooter">{xen:phrase showing_x_of_y_items, 'count=<span class="FilterListCount">{xen:count $bibles}</span>', 'total={xen:count $bibles}'}</p>

    <xen:else />
        <div class="noResults">{xen:phrase th_there_are_no_bibles_bible}</div>
    </xen:if>

</xen:form>]]></template>
