		<input type="submit" name="reload" value="{xen:phrase save_changes}" accesskey="s" class="button" id="saveReloadButton" data-ajaxvalue="{xen:phrase save_all_changes}" />
		<xen:if is="{$book.book_id}">
			<input type="button" value="{xen:phrase th_delete_book_bible}..." accesskey="d" class="button OverlayTrigger"
				data-href="{xen:adminlink bible-books/delete, $book}" />
		</xen:if>
	</xen:submitunit>
</xen:form>]]></template>
